            <li class="nav-item active">
              <%= link_to 'MAP', '/maps' , class: "nav-link" %>
            </li>
            <li class="nav-item active">
              <%= link_to 'ユーザー管理', '/users', class: "nav-link" %>
            </li>
            <li class="nav-item active">
              <%= link_to '書店登録', new_shop_path, class: "nav-link" %>
            </li>
            <li class="nav-item active">
              <%= link_to '書店管理', shops_path, class: "nav-link" %>
            </li>
          </ul> <a class="nav-item active active ml-auto">
            <%= link_to logout_path, class: "btn btn-dark", method: :delete do %>ログアウト<% end %>
                </a>
        </div>
    </nav>
  
<% end %>

 <%= yield %>
